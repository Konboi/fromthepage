h4.legend.fglight Collaborator Time by Collection

=form_tag('/dashboard/summary', method: "get") do
  =hidden_field_tag(:collection_id, @collection)
  =label_tag(:start_date, "Start date")
  |  
  =text_field_tag(:start_date, @start_deed, style: "width:30%")
  |  
  =label_tag(:end_date, "End date")
  |  
  =text_field_tag(:end_date, @end_deed, style: "width:30%")
  |  
  =submit_tag("Update")


  pre =@contributors


-content_for :javascript
=javascript_include_tag 'datepicker/datepicker'
javascript:
  //Attach a date picker for start and end dates
  datePickerController.createDatePicker({
      formElements:{
          "start_date":"%M %d, %Y"
      },
      rangeHigh: new Date(),
      noFadeEffect: true
  });

  datePickerController.createDatePicker({
      formElements:{
          "end_date":"%M %d, %Y"
      },
      rangeHigh: new Date(),
      noFadeEffect: true
  });